@inherits Umbraco.Web.Mvc.UmbracoViewPage
@using ClientDependency.Core.Mvc

@{
    Layout = "Master.cshtml";

    Html.EnableClientValidation();
    Html.EnableUnobtrusiveJavaScript();

    Html.RequiresJs("https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js");
    Html.RequiresJs("https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.7/js/bootstrap.min.js");
    Html.RequiresJs("/Scripts/jquery.validate.min.js");
    Html.RequiresJs("/Scripts/jquery.validate.unobtrusive.min.js");
    Html.RequiresJs("/Scripts/jquery.unobtrusive-ajax.min.js");
    Html.RequiresJs("/Scripts/contactForm.js");
}
@Html.Partial("~/Views/Partials/pageHeader.cshtml")


<div class="container">
    <div class="row">
        <div class="col-lg-8 col-md-10 mx-auto">
            @{
                Html.RenderAction("RenderForm", controllerName: "ContactSurface");
            }
        </div>
    </div>
</div>